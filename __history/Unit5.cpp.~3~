//---------------------------------------------------------------------------

#include <vcl.h>
#pragma hdrstop

#include "Unit5.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.dfm"
TForm5 *Form5;
double end_time = 0;
//---------------------------------------------------------------------------
__fastcall TForm5::TForm5(TComponent* Owner)
	: TForm(Owner)
{
}
//---------------------------------------------------------------------------




void __fastcall TForm5::SimulationTEditChange(TObject *Sender)
{
	if(SimulationTEdit->Text == ""){
    	end_time = 0f;
	}
	end_time = StrToFloat(SimulationTEdit->Text);
}
//---------------------------------------------------------------------------

